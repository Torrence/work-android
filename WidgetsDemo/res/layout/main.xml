<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:text="TextView" android:id="@+id/btnTextView"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	<Button android:text="Button" android:id="@+id/btnButton"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	<Button android:text="EditText" android:id="@+id/btnEditText"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	<Button android:text="RadioButton" android:id="@+id/btnRadioButton"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	<Button android:text="CheckBox" android:id="@+id/btnCheckBox"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	<Button android:text="Spinner" android:id="@+id/btnSpinner"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
</LinearLayout>
